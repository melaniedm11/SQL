SELECT * FROM players WHERE Draft_Year > 2000 ORDER BY Draft_Year;
SELECT * FROM players WHERE Field_Goal_Percentage > 0.5 ORDER BY Field_Goal_Percentage ASC; 
SELECT * FROM players WHERE Career_Points > 30000 ORDER BY Draft_Year ASC;
SELECT MAX(Points_Per_Game) AS MostPoints FROM players;
SELECT MIN(Points_Per_Game) AS LeastPoints FROM players;
SELECT AVG(Free_Throw_Percentage) FROM players;
SELECT AVG(Career_Points) FROM players;
SELECT * FROM players WHERE Team = 'LA Lakers';
SELECT * FROM players WHERE Pre_Draft_Team = 'Cincinnati';
SELECT * FROM players WHERE Pre_Draft_Team = 'Ohio State'; 
SELECT * FROM players WHERE Team IN ('Boston Celtics', 'San Antonio Spurs', 'Golden State Warriors') ORDER BY Draft_Year;
SELECT * FROM players WHERE Career_Points > 25000 AND Field_Goal_Percentage > 0.45 ORDER BY Draft_Year; 


