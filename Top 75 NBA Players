CREATE TABLE players(
    Player  TEXT,
    Team    TEXT,
    Position TEXT,
    Height  INTEGER,
    Draft_Year  INTEGER,
    Career_Points   INTEGER,
    Field_Goal_Percentage   INTEGER,
    Rebounds_Per_Game   INTEGER,
    Blocks_Per_Game INTEGER,
    Three_Point_FG_Percentage   INTEGER,
    Assists_Per_Game    INTEGER,
    Points_Per_Game INTEGER,
    Free_Throw_Percentage    INTEGER,
    Steals_Per_Game INTEGER,
    Pre_Draft_Team  TEXT,
    Last_Season     INTEGER
    
    );
    
